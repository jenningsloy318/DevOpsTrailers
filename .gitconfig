[push]
        default = simple
[core]
        editor = vim
        eol = lf
        quotepath = false
[merge]
        tool = vimdiff
[diff]
        tool = vimdiff
[difftool]
        prompt = No
[mergetool]
        prompt = No
[alias]
        st = status
        co = checkout
        br = branch
        ci = commit
        lg = "log --graph"
[format]
        pretty = format:%C(yellow)%H%Creset %C(green)%ai%Creset  %C(red)%d%Creset %C(cyan)%s%Creset   [%C(magenta)%an%Creset]