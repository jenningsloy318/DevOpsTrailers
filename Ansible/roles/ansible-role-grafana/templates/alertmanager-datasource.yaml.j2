# config file version
apiVersion: 1

# list of datasources that should be deleted from the database

# list of datasources to insert/update depending
# what's available in the database
datasources:
{% for ds in grafana_provisioning_alertmanager_datasources %}
- name: {{ ds.name }}
  type: "{{ ds.type }}"
  access: "{{ ds.access }}"
  orgId: {{ ds.orgId }}
  url: "{{ ds.url }}"
  isDefault: {{ ds.isDefault }}
  editable: {{ ds.editable }}
  version: {{ ds.version }}
  jsonData: 
    severity_critical: "{{ ds.jsonData.severity_critical }}"
    severity_high: "{{ ds.jsonData.severity_high }}"
    severity_info: "{{ ds.jsonData.severity_info }}"
    severity_warning: "{{ ds.jsonData.severity_warning }}"
{% endfor %}
