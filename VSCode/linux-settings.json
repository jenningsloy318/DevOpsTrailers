        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "cSpell.enabled": true,
    "cSpell.language": "en,cn",
    "cSpell.showStatus": true,
