{
    "editor.autoClosingBrackets": "always",
    "editor.bracketPairColorization.enabled": true,
    "editor.wordWrap": "on",
    "editor.cursorSurroundingLines": 1,
    "editor.cursorStyle": "line",
    "editor.detectIndentation": true,
    "editor.fontFamily": "'Source Code Pro','Menlo','Noto Sans Mono CJK SC'",
    "editor.fontSize": 20,
    "editor.renderLineHighlight": "none",
    "editor.lineHeight": 2,
    "editor.lineNumbers": "on",
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.pasteAs.enabled": true,
    "editor.renderWhitespace": "all",
    "editor.roundedSelection": false,
    "editor.tabSize": 2,
    "extensions.autoUpdate": true,
    "files.trimTrailingWhitespace": true,
    "git.defaultBranchName": "main",
    "go.testOnSave": true,
    "go.lintOnSave": "package",
    "go.vetOnSave": "package",
    "go.formatTool": "goimports",
    "go.testFlags": [
        "-v",
        "-coverprofile",
        "coverage.txt"
    ],
    "go.toolsGopath": "/Users/I336589/development/go",
    "go.coverOnSingleTest": true,
    "go.coverOnSave": true,
    "go.coverOnSingleTestFile": true,
    "go.coverageOptions": "showBothCoveredAndUncoveredCode",
    "go.inferGopath": true,
    "go.useLanguageServer": true,
    "go.toolsManagement.autoUpdate": true,

    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--enable-all",
        "--new"
    ],
    "go.toolsEnvVars": {
        "GOPROXY": "https://goproxy.cn",
    },
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.defaultFormatter": "golang.go",
    },
    "gopls": {
        "ui.semanticTokens": true,
    },
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": false,
        "**/.hg": true,
        "**/.history": true,
        "**/.svn": true,
        "**/.vscode": true,
        "**/bower_components": true,
        "**/CVS": true,
        "**/dist": true,
        "**/go/bin": true,
        "**/go/deps": true,
        "**/go/pkg": true,
        "**/node_modules": true,
        "**/target": true,
        "**/tmp": true
    },
    "files.watcherExclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/tmp/**": true,
        "**/bower_components": true,
        "**/dist": true,
        "**/target": true
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.openEditors.sortOrder": "alphabetical",
    "workbench.tree.indent": 15,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.fontSize": 20,
    "terminal.external.osxExec": "iTerm.app",
    "settingsSync.ignoredSettings": [
        "terminal.integrated.fontSize",
        "editor.fontSize",
        "editor.fontFamily",
        "terminal.external.linuxExec",
        "terminal.external.osxExec",
        "go.toolsEnvVars"
    ],
    "settingsSync.ignoredExtensions": [
        "alexeyvax.vscode-open-native-terminal",
        "vs-kubernetes",
        "NicolasVuillamy.vscode-groovy-lint",
        "marlon407.code-groovy"
    ],
    "settingsSync.keybindingsPerPlatform": false,
    "shell-syntax.defaultShell": "zsh",
    "leetcode.workspaceFolder": "/home/jenningsl/.leetcode",
    "leetcode.endpoint": "leetcode-cn",
    "redhat.telemetry.enabled": false,
    "search.followSymlinks": false,
    "flake8.args": [
        "--max-line-length=140",
        "--ignore=E402,F841,F401,E302,E305,E501,F405,F403",
    ],
    "autopep8.args": [
        "--max-line-length=140",
        "--ignore=E402,F841,F401,E302,E305,E501,E731",
    ],
    "files.associations": {
        "*.py": "python",
        "*.groovy": "groovy",
        "*.md": "markdown"
    },
    "terminal.external.linuxExec": "mate-terminal",
    "workbench.startupEditor": "none",
    "window.zoomLevel": -2,
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true

    },
    "markdown.math.enabled": true,
    "markdownlint.run": "onSave",
    "markdownlint.config": {
        "default": true,
        "MD003": {
            "style": "atx"
        },
        "MD007": {
            "indent": 2
        },
        "MD012": true,
        "MD018": true,
        "MD019": true,
        "MD022": true,
        "MD024": false,
        "MD025": false,
        "MD029": {
            "style": "ordered"
        },
        "MD033": false,
        "MD034": false,
        "MD036": false,
        "no-hard-tabs": false
    },
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.editorAssociations": {
        "*.docx": "default"
    },
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace"
    },
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "editor.guides.bracketPairs": true,
    "json.format.keepLines": true,
    "json.format.enable": true,
    "groovyLint.lint.trigger": "onSave",
    "groovyLint.java.options": "-Xms256m,-Xmx512m",
    "groovyLint.fix.trigger": "onSave",

    "markdown-pdf.breaks": true,
    "markdown-pdf.displayHeaderFooter": false,
    "[python]": {
        "editor.formatOnSave": true,
        //        "editor.defaultFormatter": "ms-python.autopep8"

    },

    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.windowsEnableConpty": false,
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
